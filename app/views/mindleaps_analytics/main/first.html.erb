<head>
  <script src="http://code.highcharts.com/highcharts.js"></script>
</head>
<h1>Main#first</h1>
<p>Find me in app/views/mindleaps_analytics/main/first.html.erb</p>
<%#= @series8.inspect %>
<p>
<div id="chart" style="width:100%; height:400px;"></div>
</p>

<p>
<div id="chart2" style="width:100%; height:400px;"></div>
</p>

<p>
<div id="chart4" style="width:100%; height:400px;"></div>
</p>

<p>
<div id="chart5" style="width:100%; height:400px;"></div>
</p>

<p>
<div id="chart6" style="width:100%; height:400px;"></div>
</p>

<p>
<div id="chart8" style="width:100%; height:400px;"></div>
</p>

<script>
    $(function () {
        var myChart = Highcharts.chart('chart', {
            chart: {
                type: 'scatter'
            },
            credits: {
                enabled: false
            },
            title: {
                text: '<%=t :performance_vs_time %>'
            },
            xAxis: {
                title: {
                    text: '<%=t :nr_of_lessons %>'
                }
            },
            yAxis: {
                title: {
                    text: '<%=t :performance %>'
                }
            },
            series: <%= renderSeries(@series) %>
        });

        var Chart2 = Highcharts.chart('chart2', {
            chart: {
                type: 'column'
            },
            legend: {
                enabled: false
            },
            credits: {
                enabled: false
            },
            title: {
                text: 'Number of evaluations in the MindLeaps program by month'
            },
            xAxis: {
                title: {
                    text: 'Month'
                },
                categories: <%= @x_axis2.inspect.html_safe %>
            },
            yAxis: {
                title: {
                    text: 'Nr. of Assessments'
                }
            },
            series: [{
                name: 'Nr. of Assessments',
                data: <%= @y_axis2 %>
            }]
        });

        var Chart4 = Highcharts.chart('chart4', {
            chart: {
                type: 'column'
            },
            legend: {
                enabled: false
            },
            credits: {
                enabled: false
            },
            title: {
                text: 'Histogram of student performance values'
            },
            xAxis: {
                title: {
                    text: 'Performance'
                }
            },
            yAxis: {
                title: {
                    text: 'Frequency %'
                }
            },
            series: <%= renderSeries(@series4) %>
        });

        var Chart5 = Highcharts.chart('chart5', {
            chart: {
                type: 'column'
            },
            legend: {
                enabled: false
            },
            credits: {
                enabled: false
            },
            title: {
                text: 'Performance change throughout the program by student'
            },
            xAxis: {
                title: {
                    text: 'Performance change'
                }
            },
            yAxis: {
                title: {
                    text: 'Frequency %'
                }
            },
            series: <%= renderSeries(@series5) %>
        });

        var Chart6 = Highcharts.chart('chart6', {
            chart: {
                type: 'column'
            },
            legend: {
                enabled: true
            },
            credits: {
                enabled: false
            },
            title: {
                text: 'Performance change throughout the program separated by boys and girls'
            },
            xAxis: {
                title: {
                    text: 'Performance change'
                }
            },
            yAxis: {
                title: {
                    text: 'Frequency %'
                }
            },
            series: <%= renderSeries(@series6) %>
        });

        var Chart8 = Highcharts.chart('chart8', {
            chart: {
                type: 'scatter'
            },
            legend: {
                enabled: true
            },
            credits: {
                enabled: false
            },
            title: {
                text: 'Average performance per group by days in program'
            },
            xAxis: {
                title: {
                    text: 'Days'
                }
            },
            yAxis: {
                title: {
                    text: 'Performance'
                }
            },
            series: <%= renderSeries(@series8) %>
        });
    });
</script>
